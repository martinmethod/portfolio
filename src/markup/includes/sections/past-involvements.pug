mixin involvements(data)
  each item, index in projects
    if item.category === 'involvements'
      .item
        header.cf
          +projectLine(item, index)
          span.period.meta.light #{item.date}
        footer.cf.meta
          label.light technologies & environment:
          p.dark
            each meta in item.meta
              span #{meta};
              | &nbsp;
